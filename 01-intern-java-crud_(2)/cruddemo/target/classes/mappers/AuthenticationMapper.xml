<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.java_intern.cruddemo.mapper.AuthenticationMapper">
    <select id="findByUsername" resultType="com.java_intern.cruddemo.entity.User">
        SELECT * FROM tn_user WHERE username = #{username}
    </select>
</mapper>
